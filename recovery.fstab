#
/system          ext4     /dev/block/platform/bootdevice/by-name/system       flags=display=system;backup=1;wipeingui
/data            ext4     /dev/block/platform/bootdevice/by-name/userdata     flags=display=data;backup=1;wipeingui
/cache           ext4     /dev/block/platform/bootdevice/by-name/cache        flags=display=cache;backup=1
/frp             emmc     /dev/block/platform/bootdevice/by-name/frp          flags=display=frp;backup=1
/metadata        emmc     /dev/block/platform/bootdevice/by-name/metadata     flags=display=metadata;backup=1
/tee1            emmc     /dev/block/platform/bootdevice/by-name/tee1         flags=display=tee1;backup=1
/tee2            emmc     /dev/block/platform/bootdevice/by-name/tee2         flags=display=tee2;backup=1
/expdb           emmc     /dev/block/platform/bootdevice/by-name/expdb        flags=display=expdb;backup=1
/logo            emmc     /dev/block/platform/bootdevice/by-name/logo         flags=display=logo;backup=1
/para            emmc     /dev/block/platform/bootdevice/by-name/para         flags=display=para;backup=1
/boot            emmc     /dev/block/platform/bootdevice/by-name/boot         flags=display=boot;backup=1
/recovery        emmc     /dev/block/platform/bootdevice/by-name/recovery     flags=display=recovery;backup=1
/lk              emmc     /dev/block/platform/bootdevice/by-name/lk           flags=display=lk;backup=1;
/lk2             emmc     /dev/block/platform/bootdevice/by-name/lk2          flags=display=lk2;backup=1;subpartitionof=/lk
/seccfg          emmc     /dev/block/platform/bootdevice/by-name/seccfg       flags=display=seccfg;backup=1
/persist         ext4     /dev/block/platform/bootdevice/by-name/persist      flags=display=persist;backup=1
/protect1        ext4     /dev/block/platform/bootdevice/by-name/protect1     flags=display=protect1;backup=1;
/protect2        ext4     /dev/block/platform/bootdevice/by-name/protect2     flags=display=protect2;backup=1;subpartitionof=/protect1
/nvram           emmc     /dev/block/platform/bootdevice/by-name/nvram        flags=display=nvram;backup=1
/proinfo         emmc     /dev/block/platform/bootdevice/by-name/proinfo      flags=display=proinfo;backup=1
/preloader       emmc     /dev/block/platform/bootdevice/by-name/preloader    flags=display=preloader;backup=1

# Removable storage
/external_sd     auto     /dev/block/mmcblk1p1                                flags=display="MicroSD";storage;wipeingui;removable
/usb-otg         auto     /dev/block/sda1                                     flags=display="USB OTG";storage;wipeingui;removable

# Auto-detected storage
auto             vfat     /devices/platform/soc/11130000.mmc*                 flags=display=auto
auto             vfat     /devices/platform/mt_usb*                           flags=display=auto
auto             vfat     /devices/platform/soc/11190000.usb*                 flags=display=auto

/system_image    emmc     /dev/block/bootdevice/by-name/system                flags=display="System Image"
/vendor          ext4     /dev/block/bootdevice/by-name/vendor                flags=wipeingui
/vendor_image    emmc     /dev/block/bootdevice/by-name/vendor                flags=display="Vendor Image"


